.pageIntro {
	opacity: 1;
	position: fixed;
	top: 0;
	background-color: #fff;
	z-index: 9999;
	width:100%;
	height:100%;
	div {
         position: relative;
         top: 30%;
		text-align: center;
		width:100%;
		height:100%;
		img {
			margin-top: 15px;
			width: 140px;
			height: 31px;
		}
		.goTop {
			border-top: 1px solid $borderColor;
			border-bottom: 1px solid $borderColor;
			padding: 10px;
			margin: 30px 0 0;
			cursor: pointer;
		}
	}
}

canvas{
	display: block;
	margin: 0 auto;
}
