.header {
    clear: both;
    margin: 10px 0 0;
    .headLeft {
        width: 640px;
        float: left;
        h1 {
            font-size: 11px;
            color: $borderColorDark;
            margin: 100px 0 0;
            img {
                width: 200px;
            }
            p {
                width: 400px;
                padding: 20px 0 0;
                line-height: 21px;
            }
        }
    }
    .headRight{
        width: 320px;
        float: left;
        margin: 0 0 30px;
    }
}