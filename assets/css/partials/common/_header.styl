.header {
    clear: both;
    margin: 20px 0;
    overflow: hidden;
    height: 275px;
    .headLeft {
        width: 640px;
        float: left;
        h1 {
            font-size: 12px;
            color: $borderColorDark;
            margin: 80px 0 0;
            img {
                width: 160px;
            }
            p {
                width: 400px;
                padding: 20px 0 0;
                line-height: 24px;
            }
        }
    }
    .headRight{
        width: 320px;
        float: left;
        margin: 0 0 30px;
    }
}